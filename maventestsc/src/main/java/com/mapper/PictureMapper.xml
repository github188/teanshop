<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
 <mapper namespace="com.mapper.PictureDao">
 <select id="listAll" parameterType="Picture">
 select *from myshop_picture_refund
 </select>
 <select id="getById" resultType="Picture" parameterType="int" >
  select *from myshop_picture_refund where picture_id =#{value}
 </select>
 </mapper>